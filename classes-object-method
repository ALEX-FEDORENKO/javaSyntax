//программа в которой исползуется класс Vechile 

//В этом классе обьявлется обьекта с типом Vechile 
class Vichile {
    int passenger;  //количество пассажиров
    int fuelcap; //емкость топливного бака
    int mpg; //потребление топлива в милях на галон 
}

class VechileDemo {
   public static void main(String[] args){
       Vechile minivan1 = new Vechile();
       Vechile minivan2 - new Vechile();
       int range;
       
       //присвоить значение полям в обьекте1 minivan
       minivan1.passenger = 7;
       minivan1.fuelcap = 16;
       minivan1.mpg = 24;
       
        //присвоить значение полям в обьекте2 minivan
       minivan2.passenger = 14;
       minivan2.fuelcap = 32;
       minivan2.mpg = 48;
       
       //расчитать дальность поездки при полном баке (обект 1)
       range = minivan1.fuelcap * minivan1.mpg;
       System.out.println('Мини фургон может перевезти ' + minivan1.passenger + ' пассажиров на расстояние ' + range + ' миль ');
       
       //расчитать дальность поездки при полном баке (обект 1)
       range = minivan2.fuelcap * minivan2.mpg;
       System.out.println('Мини фургон может перевезти ' + minivan2.passenger + ' пассажиров на расстояние ' + range + ' миль ');
   }
}
